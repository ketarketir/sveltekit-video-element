{
  "name": "sveltekit-video-element",
  "version": "0.0.1",
  "description": "A sveltekit video element component supporting JWPlayer, VideoJS, Cloudflare Stream and multiple video providers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuyueyi/sveltekit-video-element.git",
    "directory": "sveltekit-video-element"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.48.5",
    "@sveltejs/package": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^22",
    "@vitest/browser-playwright": "^4.0.10",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.15",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.10",
    "vitest-browser-svelte": "^2.0.1"
  },
  "keywords": [
    "svelte",
    "video",
    "jwplayer",
    "videojs",
    "sveltekit",
    "videojs-plugin",
    "sveltekit-video-element",
    "videojs-plugin-sveltekit",
    "sveltekit-video-element-plugin",
    "videojs-plugin-sveltekit-video-element",
    "sveltekit-video-element-plugin"
  ]
}